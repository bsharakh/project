<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.PrincipleCoursesMenu">
   <children>
      <Button fx:id="p_back_btn" layoutX="35.0" layoutY="30.0" mnemonicParsing="false" onAction="#go_back" prefHeight="60.0" prefWidth="120.0" text="Back">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="p_enter_btn" layoutX="636.0" layoutY="117.0" mnemonicParsing="false" onAction="#confirm_course_enter" prefHeight="60.0" prefWidth="120.0" text="Enter">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="p_clear_btn" layoutX="766.0" layoutY="117.0" mnemonicParsing="false" onAction="#clear_search" prefHeight="60.0" prefWidth="120.0" text="Clear">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="p_view_btn" layoutX="766.0" layoutY="526.0" mnemonicParsing="false" onAction="#view_course_exam" prefHeight="60.0" prefWidth="120.0" text="View">
         <font>
            <Font size="20.0" />
         </font></Button>
      <ListView fx:id="view_course" layoutX="35.0" layoutY="143.0" prefHeight="425.0" prefWidth="592.0" />
      <Label fx:id="courses_lbl" layoutX="35.0" layoutY="104.0" prefHeight="18.0" prefWidth="350.0" text="List of course exams:">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Label fx:id="enter_course_lbl" layoutX="735.0" layoutY="24.0" prefHeight="18.0" prefWidth="150.0" text="Enter course ID:">
         <font>
            <Font size="20.0" />
         </font></Label>
      <TextField fx:id="p_enter_course" layoutX="636.0" layoutY="61.0">
         <font>
            <Font size="20.0" />
         </font></TextField>
   </children>
</AnchorPane>
